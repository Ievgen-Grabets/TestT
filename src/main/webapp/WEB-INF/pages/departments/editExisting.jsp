<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title></title>
</head>
<body>
<form method="post" action="/depSaveEdited">
    <table>
        <tr>
            <td>Name:</td>
            <input type="hidden" name="id" value="${department.id}"/>
            <td><input type="text" name="name" value="${department.name}"/></td>
